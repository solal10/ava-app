{
  "dashboard": {
    "id": null,
    "title": "AVA Health Coach - Application Dashboard",
    "tags": ["ava", "health", "nodejs", "mongodb", "redis"],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Time P95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status_code=~\"4..|5..\"}[5m])",
            "legendFormat": "{{status_code}} errors"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "active_connections",
            "legendFormat": "Active connections"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Resident memory"
          },
          {
            "expr": "process_heap_bytes",
            "legendFormat": "Heap memory"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m])",
            "legendFormat": "CPU usage"
          }
        ],
        "yAxes": [
          {
            "label": "CPU cores"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "User Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_registrations_total[1h])",
            "legendFormat": "Registrations/hour"
          },
          {
            "expr": "rate(user_logins_total[1h])",
            "legendFormat": "Logins/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Health Data Submissions",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(health_data_points_total[1h])",
            "legendFormat": "{{type}} data points/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Meal Analysis Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(meal_analyses_total[1h])",
            "legendFormat": "Meal analyses/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "AI Chat Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_chat_messages_total[1h])",
            "legendFormat": "{{provider}} messages/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "Email Status",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(emails_sent_total{status=\"success\"}[1h])",
            "legendFormat": "Successful emails/hour"
          },
          {
            "expr": "rate(emails_sent_total{status=\"failed\"}[1h])",
            "legendFormat": "Failed emails/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 12,
        "title": "Garmin Sync Status",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(garmin_syncs_total{status=\"success\"}[1h])",
            "legendFormat": "Successful syncs/hour"
          },
          {
            "expr": "rate(garmin_syncs_total{status=\"error\"}[1h])",
            "legendFormat": "Failed syncs/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      }
    ],
    "schemaVersion": 16,
    "version": 0
  }
}