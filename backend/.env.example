# Configuration Base de données
MONGODB_URI=mongodb://localhost:27017/coach_sante_db
PORT=5003
NODE_ENV=development

# Configuration CORS
CORS_ORIGIN=http://localhost:5173

# Configuration Spoonacular API
SPOONACULAR_API_KEY=your_spoonacular_api_key

# Configuration JWT
JWT_SECRET=your_jwt_secret_here

# Configuration Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Configuration Garmin OAuth
GARMIN_CLIENT_ID=your_garmin_client_id
GARMIN_CLIENT_SECRET=your_garmin_client_secret
GARMIN_CONSUMER_KEY=your_garmin_consumer_key
GARMIN_CONSUMER_SECRET=your_garmin_consumer_secret

# Configuration Firebase Cloud Messaging
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY_ID=your_firebase_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_firebase_private_key\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your-project.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_firebase_client_id
FIREBASE_CLIENT_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/firebase-adminsdk-xxxxx%40your-project.iam.gserviceaccount.com

# Configuration OpenAI API (optionnel)
OPENAI_API_KEY=sk-your_openai_api_key

# Configuration Claude API (optionnel)
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key

# Configuration Email Notifications
EMAIL_PROVIDER=gmail
FROM_EMAIL=noreply@ava-coach-sante.com

# Configuration SendGrid (production recommandée)
SENDGRID_API_KEY=SG.your_sendgrid_api_key

# Configuration Gmail SMTP (développement)
GMAIL_USER=your-gmail@gmail.com
GMAIL_APP_PASSWORD=your_16_char_app_password

# Configuration AWS SES (alternative production)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=eu-west-1

# URLs Frontend pour les templates
FRONTEND_URL=http://localhost:5173

# Configuration Sentry Monitoring
SENTRY_DSN=https://your-dsn@sentry.io/project-id
SENTRY_RELEASE=ava-coach-sante@1.0.0
SENTRY_ENVIRONMENT=development